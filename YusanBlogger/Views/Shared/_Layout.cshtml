
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>@ViewData["Title"] - Yusan Blog</title>
</head>
<body>
    @RenderBody()

    <environment include="Development">
        <!-- Jquery Core Js -->
        <script src="~/plugins/jquery-3.3.1/jquery-3.3.1.min.js" asp-append-version="true"></script>
        <!-- Bootstrap Core Js -->
        <script src="~/plugins/bootstrap/js/bootstrap.min.js" asp-append-version="true"></script>
        <!-- Use SignalR Library -->
        <script src="~/js/dist/browser/signalr.min.js" asp-append-version="true"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>
